{
  "name": "neon-el",
  "type": "module",
  "version": "1.0.0",
  "description": "Web Component to give an image or element a neon glow effect.",
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "source": "src/index.js",
  "module": "dist/neon-el.js",
  "outputFormat": "esmodule",
  "includeNodeModules": [
    "dom-to-image-more"
  ],
  "context": "module",
  "alias": {
    "dom-to-image-more": "/node_modules/dom-to-image-more",
    "@swc/helpers": "/node_modules/@swc/helpers",
    "svgo": "/node_modules/svgo/dist/svgo.browser.js",
    "buffer": "/node_modules/buffer",
    "base64-js": "/node_modules/base64-js",
    "ieee754": "/node_modules/ieee754"
  },
  "scripts": {
    "start": "parcel",
    "build": "parcel build",
    "watch": "parcel watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Clive-Ward-Cron/neon.git"
  },
  "keywords": [
    "neon",
    "neon-el"
  ],
  "author": "Clive Ward-Cron <clive@ward-cron.design> (https://ward-cron.design)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Clive-Ward-Cron/neon/issues"
  },
  "homepage": "https://github.com/Clive-Ward-Cron/neon#readme",
  "devDependencies": {
    "parcel": "^2.3.2"
  },
  "dependencies": {
    "@swc/helpers": "^0.3.6",
    "buffer": "^6.0.3",
    "dom-to-image-more": "^2.9.5",
    "svgo": "^2.8.0"
  }
}
